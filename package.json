{
  "name": "l10n-populate",
  "version": "1.0.0",
  "description": "a node command line tool to inject translation into static files from json",
  "main": "index.js",
  "bin": {
    "l10n-populate": "./index.js"
  },
  "scripts": {
    "pretest": "eslint src/**/*.js tests/**/*.js index.js; exit 0;",
    "test": "tape tests/*.tests.js tests/**/*.tests.js | tap-spec",
    "watch": "npm test && chokidar 'src/***/*js' 'tests/**/*.js' 'index.js' -c 'npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurentChin/l10n-populate.git"
  },
  "keywords": [
    "l10n",
    "cli",
    "tape"
  ],
  "author": "Laurent Janet <laurent.janet@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laurentChin/l10n-populate/issues"
  },
  "homepage": "https://github.com/laurentChin/l10n-populate#readme",
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "eslint": "^4.18.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "commander": "^2.14.1"
  }
}
